class Math
{   
    int x;
    int n;
    public static void power(int x,int n)
    {
        int sum = 1;
        for (int i =1;i<=n;i++)
            sum*=x;
        System.out.println(x+"的"+n+"次方="+sum);        
    }
}

public class ex07
{
    public static void main(String args[])
    {   
        Math.power(2,5);
        Math.power(3,2);
    }    
}
